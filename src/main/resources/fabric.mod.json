{
  "schemaVersion": 1,
  "id": "tome",
  "version": "${version}",
  "name": "Tome",
  "description": "Library for creating server-side skills",
  "authors": [
    "Kouvali"
  ],
  "license": "Apache-2.0",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.kouv.tome.impl.skill.SkillEntrypoint"
    ]
  },
  "mixins": [
    "tome.mixins.json"
  ],
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_1309": [
        "org/kouv/tome/api/skill/entity/SkillSource"
      ]
    }
  },
  "depends": {
    "minecraft": ">=${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}"
  }
}